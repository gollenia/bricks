{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "ctx-blocks/header",
	"title": "Header",
	"category": "layout",
	"keywords": [ "navigation", "logo", "menu", "claim" ],
	"textdomain": "ctx-blocks",
	"description": "Contains main menu, logo and claim",
	"attributes": {
		"backgroundColor": {
			"type": "string",
			"default": "primary"
		},
		"hasImage": {
			"type": "boolean",
			"default": true
		},
		"paragraphOrientation": {
			"type": "string",
			"default": "bottom"
		},
		"mobileParagraphPosition": {
			"type": "string",
			"default": "within"
		},
		"showBreadcrumbs": {
			"type": "boolean",
			"default": true
		},
		"margin": {
			"type": "boolean",
			"default": true
		}
	},
	"editorStyle": "wp-block-alert-editor"
}
