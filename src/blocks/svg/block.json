{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ctx-blocks/svg",
	"title": "Inline SVG",
	"category": "widgets",
	"keywords": ["navigation", "logo", "menu", "claim"],
	"textdomain": "ctx-blocks",
	"description": "Inject an SVG file into the page.",
	"attributes": {
		"url": {
			"type": "string",
			"default": ""
		},
		"id": {
			"type": "number",
			"default": 0
		},
		"width": {
			"type": "number",
			"default": 500
		},
		"height": {
			"type": "number",
			"default": 500
		},
		"fillColor": {
			"type": "string",
			"default": ""
		},
		"strokeColor": {
			"type": "string",
			"default": ""
		},
		"customFillColor": {
			"type": "string",
			"default": ""
		},
		"customStrokeColor": {
			"type": "string",
			"default": ""
		},
		"strokeWidth": {
			"type": "number",
			"default": 0
		},
		"linkNewTab": {
			"type": "boolean",
			"default": false
		},
		"linkUrl": {
			"type": "string",
			"default": ""
		}
	},
	"render": "file:./render.php"
}
