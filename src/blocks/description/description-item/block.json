{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "ctx-blocks/description-item",
	"api_version": 2,
	"title": "Description Item",
	"keywords": ["list", "table"],
	"description": "Singel item of a description list",
	"textdomain": "ctx-blocks",
	"category": "common",
	"attributes": {
		"image": {
			"type": "object",
			"default": null
		},
		"title": {
			"type": "string",
			"default": ""
		},
		"icon": {
			"type": "string",
			"default": ""
		},
		"text": {
			"type": "string",
			"default": ""
		},
		"url": {
			"type": "string",
			"default": ""
		},
		"urlIcon": {
			"type": "string",
			"default": "more_horiz"
		},
		"contentType": {
			"type": "string",
			"default": ""
		},
		"roundImage": {
			"type": "boolean",
			"default": false
		},
		"seperationLines": {
			"type": "boolean",
			"default": false
		}
	},
	"parent": [ "ctx-blocks/description-list" ]
}