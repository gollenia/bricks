{
	"name": "ctx-blocks",
	"version": "1.6.0",
	"description": "Example block written with ESNext standard and JSX support – build step required.",
	"author": "kids-team Österreich",
	"license": "GPL-2.0-or-later",
	"main": "assets/index.js",
	"scripts": {
		"build": "wp-scripts build src/index.js src/frontend.js src/admin.js --output-path='./assets'",
		"format:js": "wp-scripts format-js",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"start": "wp-scripts start src/index.js src/frontend.js src/admin.js --output-path='./assets'",
		"packages-update": "wp-scripts packages-update"
	},
	"dependencies": {
		"@wordpress/block-editor": "^5.3.0",
		"@wordpress/blocks": "^11.0.0",
		"@wordpress/i18n": "^4.2.1"
	},
	"devDependencies": {
		"@wordpress/scripts": "^19.2.2",
		"postcss-cli": "^8.0.0"
	},
	"browserslist": [
		"> 1%",
		"last 3 versions",
		"not dead",
		"not ie <= 11"
	  ]
}
 
