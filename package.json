{
	"name": "bricks",
	"version": "1.6.0",
	"description": "Wordpress block library plugin to replace some standard blocks",
	"author": "kids-team Ã–sterreich",
	"prettier": "@wordpress/prettier-config",
	"license": "GPL-2.0-or-later",
	"main": "assets/index.js",
	"scripts": {
		"build": "wp-scripts build src/index.js src/frontend.js src/admin.js --output-path='./assets'",
		"format:js": "wp-scripts format-js",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"start": "wp-scripts start src/index.js src/frontend.js src/admin.js --output-path='./assets'",
		"packages-update": "wp-scripts packages-update"
	},
	"dependencies": {
		"@wordpress/block-editor": "^8.1.0",
		"@wordpress/blocks": "^11.2.0",
		"@wordpress/i18n": "^4.3.0"
	},
	"devDependencies": {
		"@wordpress/eslint-plugin": "^9.3.0",
		"@wordpress/prettier-config": "^1.1.1",
		"@wordpress/scripts": "^19.2.2",
		"eslint": "^7.32.0"
	},
	"browserslist": [
		"> 1%",
		"last 3 versions",
		"not dead",
		"not ie <= 11"
	]
}
